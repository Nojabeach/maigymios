{
  "appId": "com.vitality.app",
  "appName": "Vitality",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#22c55e",
      "androidSplashResourceName": "splash",
      "iosSplashResourceName": "Default"
    }
  }
}
